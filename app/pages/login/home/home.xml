<Page xmlns="http://schemas.nativescript.org/tns.xsd" xmlns:collapsingHeader="nativescript-collapsing-header" xmlns:Slides="nativescript-slides" xmlns:Card="nativescript-cardview" xmlns:td="nativescript-textdrawable" loaded="homeLoaded" unloaded="homeUnLoaded">
  <ActionBar title="INK Connect">
    <ActionItem tap="settings_action" icon="~/images/ic_settings_white_36dp.png" android.position="actionBar"></ActionItem>
  </ActionBar>
  <TabView class="whitetext fontsymbol font20" id="tab_view_id" selectedColor="#FFFFFF" tabsBackgroundColor="#21409A">
    <TabView.items>
      <TabViewItem title="&#xf1ea;" class="" xmlns:NewsFeedTab="pages/login/home/newsfeed">
        <TabViewItem.view>
          <NewsFeedTab:newsfeed />
        </TabViewItem.view>
      </TabViewItem>
      <TabViewItem title="&#xf073;" class="" xmlns:ScheduleTab="pages/login/home/schedule">
        <TabViewItem.view>
          <ScheduleTab:schedule/>
        </TabViewItem.view>
      </TabViewItem>
      <TabViewItem title="&#xf086;" class="" xmlns:ChatTab="pages/login/home/chat">
        <TabViewItem.view>
          <ChatTab:chat/>
        </TabViewItem.view>
      </TabViewItem>
      <TabViewItem title="&#xf0c0;" class="" xmlns:SpeakersTab="pages/login/home/speakers">
        <TabViewItem.view>
          <SpeakersTab:speakers/>
        </TabViewItem.view>
      </TabViewItem>
    </TabView.items>
  </TabView>
</Page>
